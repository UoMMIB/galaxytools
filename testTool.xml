<tool id="testTool" name="TestTool" version="0.1.0">
    <description>performs Galaxy tests.</description>
    <command detect_errors="exit_code"><![CDATA[
        python3 $__tool_directory__/testTool.py -o $output1 -p $output_1.extra_files_path
    ]]></command>
    <outputs>
        <data name="output1" format="html" from_work_dir="out.html" />
    </outputs>
</tool>

<tool id="testTool" name="TestTool" version="0.1.0">
    <description>performs Galaxy tests.</description>
    <command detect_errors="exit_code"><![CDATA[
        python3 $__tool_directory__/testTool.py -o $output1 -p $output1.extra_files_path
    ]]></command>
    <outputs>
        <data name="output1" format="html" label="myToolOutput_${tool_name}.html"/>
    </outputs>
</tool>
